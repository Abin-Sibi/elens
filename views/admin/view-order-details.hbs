 <div class="height-100 bg-light">
<table id="example" class="display nowrap" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Product Name</th>
                <th>Category</th>
                <th>Price</th>
                <th>DiscountPrice</th>
                <th>Payment Mode</th>
                <th>Status</th>
                <th>pinCode</th>
                <th>Address</th>
                <th>City</th>

            </tr>
        </thead>
        {{#each order}}
        <tbody>
            <tr>
                
                <td>{{this.Nameuser}}</td>
                <td>{{this.email}}</td>
                <td>{{this.name}}</td>
                <td>{{this.category}}</td>
                <td>{{this.totalAmount}}</td>
                <td>{{this.discountPrice}}</td>
                <td>{{this.payment}}</td>
                <td>{{this.status}}</td>
                <td>{{this.pincode}}</td>
                <td>{{this.address}}</td>
                <td>{{this.city}}</td>
            </tr>
        </tbody>
        {{/each}}
</table>
 </div>